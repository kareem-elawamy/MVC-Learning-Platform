@model ONE.Models.Video
@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label for="VideoId">Video ID</label>
        <input asp-for="VideoId" class="form-control" required />
    </div>
    <div class="form-group">
        <label asp-for="MaterialID" class="control-label"></label>
        <select asp-for="MaterialID" class="form-control" asp-items="ViewBag.MaterialID"></select>
    </div>

    <button type="submit" class="btn btn-primary">Add</button>
</form>